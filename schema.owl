<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/company_ontology.owl"
         xmlns="http://example.org/company_ontology.owl#">

<owl:Ontology rdf:about="http://example.org/company_ontology.owl"/>

<owl:ObjectProperty rdf:about="#isProductExperOf">
  <rdfs:domain rdf:resource="#Employee"/>
  <rdfs:range rdf:resource="#Product"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasTitle">
  <rdfs:domain rdf:resource="#Person"/>
  <rdfs:range rdf:resource="#ProductManager"/>
  <rdfs:range rdf:resource="#Manager"/>
  <rdfs:range rdf:resource="#Employee"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasManager">
  <rdfs:domain rdf:resource="#Department"/>
  <rdfs:range rdf:resource="#Manager"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasEmployee">
  <rdfs:domain rdf:resource="#Department"/>
  <rdfs:range rdf:resource="#Employee"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#offersProduct">
  <rdfs:domain rdf:resource="#Department"/>
  <rdfs:range rdf:resource="#ProductID"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#offersService">
  <rdfs:domain rdf:resource="#Department"/>
  <rdfs:range rdf:resource="#ServiceID"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasRelatedProduct">
  <rdfs:domain rdf:resource="#Product"/>
  <rdfs:range rdf:resource="#Product"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasProduct">
  <rdfs:domain rdf:resource="#Service"/>
  <rdfs:range rdf:resource="#ProductID"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasProductManager">
  <rdfs:domain rdf:resource="#Service"/>
